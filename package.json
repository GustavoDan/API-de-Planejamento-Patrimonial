{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch --env-file .env src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint src --ext .ts --fix",
    "db:migrate": "npx prisma migrate dev",
    "db:studio": "npx prisma studio",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate deploy",
    "test:seed": "dotenv -e .env.test -- npx prisma db seed",
    "test:run": "dotenv -e .env.test -- jest --runInBand",
    "test:coverage": "npm run test:migrate && npm run test:run -- --coverage",
    "db:seed:prod": "node dist/prisma/seed.js"
  },
  "keywords": [],
  "author": "",
